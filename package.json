{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --fix",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy:production": "gatsby build && gh-pages -d public -b master -r git@github.com:vitormalencar/vitormalencar.github.io",
    "deploy:staging": "yarn build && surge public -d 'https://psychotic-sister.surge.sh'",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^4.8.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-cname": "^1.0.0",
    "gatsby-plugin-manifest": "^4.8.1",
    "gatsby-plugin-offline": "^5.8.1",
    "gatsby-plugin-postcss": "^5.8.0",
    "gatsby-plugin-react-helmet": "^5.8.0",
    "gatsby-plugin-sharp": "^4.8.1",
    "gatsby-source-filesystem": "^4.8.1",
    "gatsby-transformer-sharp": "^4.8.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-reveal": "^1.2.2",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-plugin-emotion": "^7.8.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss-selector-parser": "^6.0.9",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
